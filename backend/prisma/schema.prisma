generator client {
  provider = "go run github.com/steebchen/prisma-client-go"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Admin {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}

model Bio {
  id          Int    @id @default(autoincrement())
  dp          String
  name        String
  intro       String
  description String
}

model ProgrammingLang {
  languageName String @unique
}

model Tool {
  toolName String @unique
}

model Database {
  databaseName String @unique
}
